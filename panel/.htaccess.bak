<IfModule mod_rewrite.c>
  RewriteEngine On
  # Ustaw bazę na podkatalog, w którym znajduje się aplikacja React
  RewriteBase /panel/
  # Nie stosuj reguł do istniejących plików
  RewriteCond %{REQUEST_FILENAME} !-f
  # Nie stosuj reguł do istniejących katalogów
  RewriteCond %{REQUEST_FILENAME} !-d
  # Przekieruj wszystkie inne żądania do index.html
  RewriteRule . /panel/index.html [L]
</IfModule>
